<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.backend.mapper.ArticleMapper">
    <select id="getTagArticle" resultType="com.backend.entity.ArticleEntity">
        select  article.*  from article , article_tag
        where article.articleid = article_tag.articleid
     </select>
</mapper>
